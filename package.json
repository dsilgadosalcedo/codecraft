{
  "name": "codehere",
  "private": true,
  "type": "module",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "vite",
    "start": "vite --port 3000",
    "build": "tsc -b && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "quality": "bun run type-check && bun run lint && bun run format:check && bun run test:run",
    "prepare": "husky",
    "docs": "typedoc",
    "docs:serve": "typedoc --serve",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.159.5",
    "@tanstack/react-router-devtools": "^1.159.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "emmet-monaco-es": "^5.6.1",
    "jszip": "^3.10.1",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.563.0",
    "monaco-editor": "^0.55.1",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-split-grid": "^1.0.4",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^7.2.0",
    "@eslint/js": "^10.0.1",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/react": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "storybook": "^10.2.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/ui": "^4.0.18",
    "cypress": "^15.10.0",
    "eslint": "^10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "web-vitals": "^5.1.0",
    "typedoc": "^0.28.16"
  },
  "overrides": {
    "@tanstack/react-router": "^1.159.5",
    "form-data@>=4.0.0 <4.0.4": ">=4.0.4",
    "@eslint/plugin-kit@<0.3.4": ">=0.3.4",
    "tmp@<=0.2.3": ">=0.2.4",
    "js-yaml@*": "^4.1.1",
    "qs@*": ">=6.14.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "bun@1.3.9"
}
